{
  "manifest_version": 3,
  "name": "Voice Transcriber",
  "version": "1.0.0",
  "description": "Real-time voice transcription with auto-insert",

  "permissions": ["clipboardWrite"],

  "host_permissions": ["http://localhost:8000/*"],

  "action": {
    "default_popup": "index.html",
    "default_title": "Voice Transcriber"
  }
}
